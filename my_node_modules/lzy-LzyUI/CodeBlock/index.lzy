import LzyReact, { myUseEffect } from 'lzy-react'
import './prettify.js'


export default function CodeBlock(props, children) {

    const { codeStr } = props

    // 初始化代码高亮
    myUseEffect(() => {
        window.prettyPrint()
    })

    return (
        <pre class="pre-scrollable prettyprint linenums">
            {codeStr}
        </pre>
    );
}
